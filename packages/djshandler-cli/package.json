{
  "name": "djshandler-cli",
  "version": "1.0.0",
  "description": "CLI tool for generating and managing Discord.js bot projects with djshandler",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "djshandler": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "discord",
    "discord.js",
    "cli",
    "generator",
    "scaffold",
    "command-handler"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "djshandler-core": "^1.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.17",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
